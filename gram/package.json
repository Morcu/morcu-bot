{
  "name": "GRAM",
  "version": "2.0.0",
  "description": "A Grammar Recognizer",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run compile && npm run gram2lib",
    "clean": "shx rm -fR ./lib",
    "compile": "tsc",
    "dev": "npm run build && npm run watch",
    "info": "npm-scripts-info",
    "lint": "tslint './src/**/*.ts'",
    "prepublish": "npm run build",
    "security": "nsp check",
    "test": "# npm run build && mocha -R spec 'lib/**/*.spec.js'",
    "watch": "npm-run-all --parallel watch:*",
    "watch:ts": "tsc -w",
    "watch:js": "nodemon --watch lib --exec node lib/app.js",
    "start": "node lib/app.js",
    "autotest": "node lib/e2e/DLTest.js",
    "debug": "node --debug-brk --inspect  lib/app.js",
    "webdebug": "node --debug-brk lib/app.js",
    "gram2lib": "shx mkdir -p ./lib/cs; shx cp -rf ./src/cs/gram/gen ./lib/cs/gram",
    "testgram": "node ./lib/test/testRecognizer.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Node.js",
    "elasticsearch"
  ],
  "author": "",
  "license": "",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "scripts-info": {
    "info": "List available scripts",
    "build": "Builds the plugin, by cleaning and compiling TypesScript into Javascript",
    "compile": "Just compiles your code",
    "dev": "Watch codebase, trigger compile when source code changes",
    "lint": "Check coding guidelines conformance",
    "security": "Check if your dependencies are secure",
    "test": "Test your codebase"
  },
  "dependencies": {
    "antlr4": "^4.7.0",
    "botbuilder": "^3.14.1",
    "dotenv": "^2.0.0",
    "jsonfile": "^3.0.0",
    "logops": "^1.0.7",
    "mongodb": "^2.2.25",
    "request": "^2.79.0",
    "request-promise": "4.1.1",
    "restify": "^7.2.1",
    "typescript-collections": "^1.2.3"
  },
  "devDependencies": {
    "@types/chai": "^3.4.33",
    "@types/dotenv": "^2.0.19",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.41",
    "@types/request": "0.0.36",
    "@types/request-promise": "4.1.33",
    "@types/sinon": "^1.16.30",
    "@types/sinon-chai": "^2.7.27",
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "mocha": "^3.0.2",
    "nodemon": "^1.9.2",
    "npm-run-all": "^3.1.0",
    "npm-scripts-info": "^0.3.6",
    "nsp": "^2.6.1",
    "shx": "^0.1.4",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "tslint": "^3.15.1",
    "typescript": "^2.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "typings": "./lib/index.d.ts"
}
